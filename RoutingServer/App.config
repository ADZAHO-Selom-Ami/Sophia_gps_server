<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<bindings>
   <basicHttpBinding>
    <binding name="BasicHttpBinding_IProxy" />
   </basicHttpBinding>
  </bindings>
  <client>
   <endpoint address="http://localhost:8733/Design_Time_Addresses/Proxy/"
    binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProxy"
    contract="ProxyService.IProxy" name="BasicHttpBinding_IProxy" />
  </client>
  <services>
			<service name="RoutingServer.Server">
				<endpoint address="http://localhost:8733/Design_Time_Addresses/RoutingServer" binding="webHttpBinding" contract="RoutingServer.IServer"/>
			</service>
		</services>
		<behaviors>
			<endpointBehaviors>
				<behavior>
					<webHttp/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>

